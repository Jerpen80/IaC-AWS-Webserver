                     function WillekeurigGetal(max)
                    {
                    return (Math.floor(Math.random()*max))+1;
                    
                    }
                     function getal()  
                          { 
                         WillekeurigGetal(10) 
                      keuze = document.formulier.nummers.selectedIndex
                      var cijfer=document.formulier.nummers[keuze].value;
                      minder=WillekeurigGetal(10);
                      goed=minder-1;
                      if (keuze==goed)
                       {alert ('Goed zo!! het getal was inderdaad '+minder);}
                       else
                       {alert ('jammer.....het getal was '+minder);}
                      
                      }
                    
                     
                     
                     
                      </SCRIPT> 
                    
                     
                     
                     
